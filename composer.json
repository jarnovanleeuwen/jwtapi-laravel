{
    "name": "jarnovanleeuwen/jwtapi-laravel",
    "type": "library",
    "description": "JWT API authentication driver (Guard) for Laravel.",
    "keywords": [
        "jwt",
        "api",
        "json",
        "laravel",
        "authentication",
        "guard"
    ],
    "homepage": "https://github.com/jarnovanleeuwen/jwtapi-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jarno van Leeuwen",
            "email": "vanleeuwen.jarno@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/auth": "~5.5|~5.6",
        "illuminate/contracts": "~5.5|~5.6",
        "illuminate/database": "~5.5|~5.6",
        "illuminate/http": "~5.5|~5.6",
        "illuminate/support": "~5.5|~5.6",
        "jarnovanleeuwen/jwtapi": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "JwtApi\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JwtApi\\Laravel\\JwtApiServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
